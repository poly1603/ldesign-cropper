{
  "name": "@ldesign/cropper-workspace",
  "version": "2.0.0",
  "private": true,
  "description": "A powerful, high-performance image cropper library with advanced filters, batch processing, and drawing tools. Works with any framework.",
  "type": "module",
  "scripts": {
    "dev": "echo 'Please run dev in specific package'",
    "build": "pnpm -r build",
    "build:core": "pnpm --filter @ldesign/cropper-core build",
    "build:vanilla": "pnpm --filter @ldesign/cropper build",
    "build:vue": "pnpm --filter @ldesign/cropper-vue build",
    "build:react": "pnpm --filter @ldesign/cropper-react build",
    "build:lit": "pnpm --filter @ldesign/cropper-lit build",
    "demo:vanilla": "pnpm --filter vanilla-cropper-demo dev",
    "demo:vue": "pnpm --filter vue-cropper-demo dev",
    "demo:react": "pnpm --filter react-cropper-demo dev",
    "demo:lit": "pnpm --filter lit-cropper-demo dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r --parallel run clean",
    "install:all": "pnpm install"
  },
  "keywords": [
    "cropper",
    "image-cropper",
    "crop",
    "image",
    "image-editor",
    "image-filters",
    "batch-processing",
    "vue",
    "react",
    "lit",
    "web-components",
    "typescript",
    "performance",
    "filters",
    "instagram-filters",
    "watermark",
    "drawing",
    "annotations"
  ],
  "author": "ldesign",
  "license": "MIT"
}