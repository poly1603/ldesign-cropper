{
  "name": "@ldesign/cropper",
  "type": "module",
  "version": "2.0.0",
  "description": "A powerful, high-performance image cropper library - Vanilla JS wrapper",
  "author": "ldesign",
  "license": "MIT",
  "keywords": [
    "cropper",
    "image-cropper",
    "vanilla-js",
    "typescript"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.less"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "types": "./es/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "es",
    "lib",
    "package.json"
  ],
  "scripts": {
    "dev": "ldesign-builder --watch",
    "build": "ldesign-builder build",
    "clean": "rimraf es lib dist",
    "lint": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@ldesign/cropper-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
