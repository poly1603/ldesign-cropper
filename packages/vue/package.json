{
  "name": "@ldesign/cropper-vue",
  "version": "2.0.0",
  "description": "Vue 3 adapter for @ldesign/cropper-core",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "test": "vitest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "cropper",
    "vue",
    "vue3",
    "image-cropper",
    "composition-api",
    "directive"
  ],
  "author": "ldesign",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "@ldesign/cropper-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@ldesign/cropper-core": "workspace:*"
  }
}

