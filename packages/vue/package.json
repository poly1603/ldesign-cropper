{
  "name": "@ldesign/cropper-vue",
  "type": "module",
  "version": "2.0.0",
  "description": "Vue 3 wrapper for @ldesign/cropper",
  "author": "ldesign",
  "license": "MIT",
  "keywords": [
    "cropper",
    "image-cropper",
    "vue",
    "vue3",
    "component"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.less",
    "**/*.vue"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./es/Cropper.vue.css": "./es/Cropper.vue.css",
    "./lib/Cropper.vue.css": "./lib/Cropper.vue.css"
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "types": "./es/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "es",
    "lib",
    "package.json"
  ],
  "scripts": {
    "dev": "ldesign-builder --watch",
    "build": "ldesign-builder build",
    "clean": "rimraf es lib dist",
    "lint": "tsc --noEmit",
    "test": "vitest"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@ldesign/cropper-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "vue": "^3.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
